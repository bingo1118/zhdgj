        android:layout_height="50dp"
        android:background="@drawable/message_banner"
        android:layout_alignParentBottom="true">
        <ImageView
            android:id="@+id/setting_dev_img"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@drawable/setting_eletc"
            android:layout_centerVertical="true"
            android:layout_marginLeft="50dp"
            android:layout_marginTop="3dp"/>

        <ImageView
            android:id="@+id/share_dev_img"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@drawable/share_electr"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="3dp"/>
        <ImageView
            android:id="@+id/timer_img"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:background="@drawable/timer_electr"
            android:layout_marginRight="50dp"
