            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="区域:"
            android:textSize="16sp"
            android:layout_margin="5dp"/>
        <TextView
            android:id="@+id/dev_address"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="地址:"
            android:singleLine="true"
            android:ellipsize="end"
            android:textSize="16sp"
            android:layout_margin="5dp"/>
    </LinearLayout>

