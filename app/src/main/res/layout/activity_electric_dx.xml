<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5f6f6">



    <ImageView
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:adjustViewBounds="true"
        android:scaleType="centerCrop"
        android:src="@drawable/message_banner"/>

    <ImageView
        android:id="@+id/swipe_fresh_layout"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_margin="15dp"
        android:layout_alignTop="@id/title_include"
        android:background="@drawable/refrash"/>

    <!-- title bar -->
    <include android:id="@+id/title_include" layout="@layout/title_bar"/>

    <LinearLayout
        android:id="@+id/info_linear"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="15dp"
        android:layout_below="@id/title_include"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:background="@drawable/login_account_and_psw_shape"
        android:orientation="vertical">
        <TextView
            android:id="@+id/dev_id"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="SN码:"
            android:textSize="16sp"
            android:layout_margin="5dp"/>
        <TextView
            android:id="@+id/dev_areaid"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="区域:"
            android:textSize="16sp"
            android:layout_margin="5dp"/>
        <TextView
            android:id="@+id/dev_address"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="地址:"
            android:singleLine="true"
            android:ellipsize="end"
            android:textSize="16sp"
            android:layout_margin="5dp"/>
    </LinearLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/info_linear"
        android:layout_marginBottom="60dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="10dp"
        android:padding="5dp"
        android:background="@drawable/login_account_and_psw_shape">
